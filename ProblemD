#include <iostream>
#include <algorithm>
using namespace std;
void solve()
{
    int n;
    cin >> n;
    int a[n];
    for (int i = 0; i < n; i++)
    {
        cin >> a[i];
    }
    int dp[n];
    dp[0] = 0;
    int maxValue = 0;
    int run = a[0];
    for (int i = 1; i < n;)
    {
        int cnt = i;
        while (cnt < n && run--)
        {
            dp[cnt] = dp[i - 1] + 1;
            if (a[cnt] - run > maxValue)
            {
                maxValue = a[cnt] - run;
            }
            cnt++;
        }
        if (cnt == i)
        {
            cout << "CANNOT GET DESTINATION" << endl;
            return;
        }
        run = maxValue;
        maxValue = 0;
        i = cnt;
    }
    cout << dp[n - 1] << endl;
}
int main()
{
    solve();
}
